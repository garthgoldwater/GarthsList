<section class="outer-container">
<h2 class="page-title">Admin Panel</h2>
<div class="form">
  <%= form_for @region do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %>
    <%= form.submit "Add new region" %>
  <% end %>
</div>
<div class="form">
  <%= form_for @category do |f| %>
    <%= f.label "Name:", :name %>
    <%= f.text_field :name %>
    <%= f.submit %>
  <% end %>
</div>
